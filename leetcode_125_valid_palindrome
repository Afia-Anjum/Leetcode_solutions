class Solution:
    def isPalindrome(self, s: str) -> bool:
        s=s.lower()
        s="".join(s.split())
        new_s=[]
        for i in range(len(s)):
            if s[i].isalnum():
                new_s.append(s[i])
        new_s="".join(new_s)    
        print(new_s)
        low=0
        high=len(new_s)-1
        flag=0
        while(low<high):
            if new_s[low]==new_s[high]:
                low+=1
                high-=1
            else:
                flag=1
                break
        if flag==0:
            return True
        else:
            return False
