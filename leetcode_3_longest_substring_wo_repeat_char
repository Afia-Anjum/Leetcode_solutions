from collections import defaultdict
class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        dict=defaultdict(int)
        left=right=0
        max_length=0
        for right in range(len(s)):
            print(s[left:right])
            if s[right] in dict:
                dict[s[right]]+=1
            else:
                dict[s[right]]=1
            while dict[s[right]]>1:
                dict[s[left]]-=1
                left+=1
            max_length=max(max_length,right-left+1)
        return max_length
