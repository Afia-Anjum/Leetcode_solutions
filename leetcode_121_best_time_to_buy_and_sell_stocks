class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        min_buy_price=10000
        max_sell_price=0
        flag=0
        for i in range(len(prices)):
            if prices[i]<min_buy_price:
                min_buy_price=prices[i]
                flag=1
                continue
            if flag==1:
                if prices[i]-min_buy_price>max_sell_price:
                    max_sell_price=prices[i]-min_buy_price
        return max_sell_price
            
