class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        dict_ransom=defaultdict(int)
        dict_magazine=defaultdict(int)

        for i in range(len(ransomNote)):
            dict_ransom[ransomNote[i]]+=1
        for i in range(len(magazine)):
            dict_magazine[magazine[i]]+=1
        
        flag=0
        for key in dict_ransom.keys():
            if dict_magazine[key]>=1:
                dict_magazine[key]=dict_magazine[key]-dict_ransom[key]
                if dict_magazine[key]>=0:
                    flag=1
                else:
                    flag=0
                    break
            else:
                flag=0
                break

        if flag==1:
            return True
        else:
            return False


